{
  "comments": false,
  "plugins": ["@babel/transform-runtime"],
  "presets": [
    [
      "@babel/env",
      {
        "targets": { "node": true },
        "modules": false,
        "shippedProposals": true,
        "loose": true
      }
    ],
    [
      "@babel/react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "overrides": [
    {
      "test": "**/*.js",
      "sourceType": "script"
    }
  ]
}
