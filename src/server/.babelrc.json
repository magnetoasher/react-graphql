{
  "comments": false,
  "plugins": ["@babel/transform-runtime"],
  "presets": [
    [
      "@babel/env",
      {
        "targets": "Node 10.17 - 11 and Node < 11, Node 12 - 13 and Node < 13, Node >= 13.7",
        "modules": false,
        "shippedProposals": true,
        "loose": true
      }
    ]
  ],
  "overrides": [
    {
      "test": "**/*.js",
      "sourceType": "script"
    }
  ]
}
