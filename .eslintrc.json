{
  "extends": ["env"],
  "settings": {
    "jsdoc": {
      "mode": "typescript"
    },
    "polyfills": [
      "AbortController",
      "CustomEvent",
      "Event",
      "EventTarget",
      "fetch",
      "FormData",
      "performance"
    ]
  },
  "rules": {
    "jsdoc/require-description": "off",
    "jsdoc/require-returns": "off",
    "jsdoc/valid-types": "off"
  }
}
